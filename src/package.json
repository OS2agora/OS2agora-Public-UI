{
  "name": "hearing-portal-public",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "predev": "sh ./scripts/predev.sh",
    "dev": "react-env --prefix NEXT_PUBLIC_EXT -- next dev --turbopack",
    "build": "next build",
    "start": "react-env --prefix NEXT_PUBLIC_EXT -- next start",
    "storybook": "storybook dev -p 9001",
    "build-storybook": "storybook build",
    "build:icons": "npx @svgr/cli --icon -d components/icons/components components/icons/svgs",
    "clean-svgs": "svgo -f components/icons/svgs --config=.svgo.yml",
    "prettier": "cross-env ./node_modules/.bin/prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "cross-env ./node_modules/.bin/prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "linting": "eslint .",
    "linting:fix": "eslint --fix ."
  },
  "dependencies": {
    "@beam-australia/react-env": "3.1.1",
    "@tailwindcss/postcss": "4.0.9",
    "axios": "1.8.1",
    "clsx": "1.1.1",
    "formik": "2.2.6",
    "html-to-react": "1.4.5",
    "i18next": "24.2.2",
    "js-cookie": "3.0.5",
    "next": "15.1.7",
    "next-i18next": "15.4.2",
    "postcss": "8.5.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "11.3.1",
    "react-i18next": "15.4.1",
    "react-markdown": "5.0.3",
    "react-query": "3.12.0",
    "tailwindcss": "4.0.9"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.2.0",
    "@storybook/addon-essentials": "8.6.2",
    "@storybook/addon-knobs": "8.0.1",
    "@storybook/addon-links": "8.6.2",
    "@storybook/addon-styling-webpack": "1.0.1",
    "@storybook/addon-webpack5-compiler-babel": "3.0.5",
    "@storybook/nextjs": "8.6.2",
    "@storybook/react": "8.6.2",
    "@svgr/cli": "5.5.0",
    "@types/js-cookie": "3.0.6",
    "@types/node": "22.13.9",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "7.23.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.2.1",
    "storybook": "8.6.2",
    "svgo": "1.3.2",
    "typescript": "5.8.2"
