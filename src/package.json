{
  "name": "hearing-portal-public",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "react-env --prefix NEXT_PUBLIC_EXT -- next dev",
    "build": "next build",
    "start": "react-env --prefix NEXT_PUBLIC_EXT -- next start",
    "storybook": "start-storybook -s ./public -p 9001",
    "build-storybook": "build-storybook",
    "build:icons": "npx @svgr/cli --icon -d components/icons/components components/icons/svgs",
    "clean-svgs": "svgo -f components/icons/svgs --config=.svgo.yml",
    "prettier": "cross-env ./node_modules/.bin/prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "cross-env ./node_modules/.bin/prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "linting": "eslint .",
    "linting:fix": "eslint --fix ."
  },
  "dependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "@tailwindcss/line-clamp": "0.2.0",
    "autoprefixer": "10.2.4",
    "axios": "0.21.1",
    "clsx": "1.1.1",
    "formik": "2.2.6",
    "html-to-react": "^1.4.5",
    "next": "10.0.6",
    "postcss": "8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-dropzone": "11.3.1",
    "react-markdown": "5.0.3",
    "react-query": "3.12.0",
    "tailwindcss": "2.0.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.2.0-alpha.24",
    "@storybook/addon-knobs": "6.2.0-alpha.24",
    "@storybook/addon-links": "6.2.0-alpha.24",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.2.0-alpha.24",
    "@svgr/cli": "5.5.0",
    "@types/react": "18.0.1",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.2.1",
    "svgo": "1.3.2",
    "typescript": "4.1.5"
  }
}
